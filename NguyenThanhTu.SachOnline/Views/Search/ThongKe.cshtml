@model IEnumerable<NguyenThanhTu.SachOnline.Models.ReportInfo>

@{
    ViewBag.Title = "ThongKe";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<h2>THỐNG KÊ SỐ ADIDAS</h2>
<table border="1">
    <tr style="background-color:burlywood; font-weight:bold">
        <td>Mã Adidas</td>
        <td>Số lượng</td>
        <td>Tổng số giày</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Count</td>
            <td>@item.Sum</td>
        </tr>
     }
</table>

